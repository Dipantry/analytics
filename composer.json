{
  "name": "dipantry/analytics",
  "description": "Analytics for Dipantry usages",
  "keywords": [
    "laravel",
    "laravel-analytics",
    "analytics",
    "middleware",
    "package"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "illuminate/support": "^9.0.0|^10.0.0",
    "mobiledetect/mobiledetectlib": "^3.74.0",
    "jaybizzle/crawler-detect": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1",
    "php-coveralls/php-coveralls": "^2.1",
    "orchestra/testbench": "^7.0",
    "ext-openssl": "*",
    "ext-intl": "*"
  },
  "authors": [
    {
      "role": "Developer",
      "name": "Auriga Aristo",
      "email": "aurigaaristo@gmail.com",
      "homepage": "https://aurigaaristo.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Dipantry\\Analytics\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dipantry\\Analytics\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
